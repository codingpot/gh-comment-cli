{
  "manifest_version": 3,
  "name": "GHC CLI",
  "version": "0.0.1",
  "description": "GitHub Comment as CLI",
  "action": {
    "default_title": "GHC CLI"
  },
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ]
}
