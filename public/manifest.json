{
  "manifest_version": 3,
  "name": "GHC CLI",
  "version": "1.0.0",
  "description": "This chrome extension convert comment sections in GitHub as CLI",
  "action": {
    "default_title": "GHC CLI"
  },
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["contentScript.js"],
      "css": ["contentStyle.css"]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
